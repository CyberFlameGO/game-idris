{
  "states": ["before", "after", "garbage"],
  "start": "before",
  "end": "garbage",

  "before": {
    "onCollision": {
      "transition": {
        "state": "after",
        "actions": [
          {"type": "projectile damage"},
          {"type": "add mask bit", "maskBit": "projectile"}
        ]
      }
    }
  },

  "after": {
    "onTime": {
      "time": 5,
      "time_parameter": "garbage_time",
      "transition": {
        "state": "garbage"
      }
    }
  },

  "garbage": {}
}
